<div class="container">
	
	
	<div class="row">
	    <div class="col-sm-7">
			<br>
			<img src="./images/logo.jpg" alt="Mini Library Book Tracker">      
	    </div>
		<div class="col-sm-5 title">
			<h1><b>Mini Library Book Tracker</b></h1>
	        <p><i>by Team Book Slingers</i></p>
	        <br>
	        <p class="right"><a href="/admin">Admin</a>  |  <a href="/usercrud">Checkout List</a>  |  <a href="/crud">Add Books<a/>  |  <a href="/checkout">Checkout Book</a></p>
	    </div>
  	</div>
	<hr>
	<div class="row">	

	    <div class="col-sm-2">
	    </div>
  	</div>
	
	<div class=""><h2>Checkout List</h2>
				<br>
	    <div class="col-sm-8">

			<div class="container scroll">
				<table class="table table-hover table-nonfluid">
				    <thead>
					     <tr>
					        <th>Title</th>
					        <th>Name</th>
					        <th>User NetID</th>
					        <th>Checkout</th>
					        <th>Due Date</th>
					     </tr>
					</thead>					    
					<tbody class="">
						{{#each users}}
					 		<tr tabindex="1">
					 			<td></td>
						        <td>{{this.name}}</td>
						        <td>{{this.netID}}</td>
						        <td>{{this.checkout_date}}</td>
						        <td>{{this.due_date}}</td>						       
						      </tr>
						{{/each}}
					</tbody>
			 	</table>
			</div>

		</div>

	</div>
	
	<div class="col-sm-2">
    </div>

</div> 

<BR>